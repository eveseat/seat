{
  "name": "eveseat/seat",
  "description": "Simple Eve Api Tool",
  "keywords": [
    "eve",
    "online",
    "corporation",
    "management",
    "api"
  ],
  "license": "GPL-2.0",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "fideloper/proxy": "^4.0",
    "fruitcake/laravel-cors": "^2.0",
    "laravel/framework": "^8.0",
    "laravel/tinker": "^2.6",
    "eveseat/api": "5.0.x-dev",
    "eveseat/eveapi": "5.0.x-dev",
    "eveseat/notifications": "5.0.x-dev",
    "eveseat/services": "5.0.x-dev",
    "eveseat/web": "5.0.x-dev",
    "ext-pdo": "*"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Seat\\Core\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
